    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "tumor =  pd.read_excel(\"data/medicaldata_tumorgrowth.xlsx\")\n",
    "\n",
    "(\n",
    "tumor\n",
    "  .loc[tumor[\"Day\"] == 21]\n",
    "  .groupby(\"Group\")\n",
    "  .agg(avg = (\"Size\", np.mean))\n",
    ")"
